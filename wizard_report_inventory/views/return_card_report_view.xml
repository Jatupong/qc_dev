<odoo>
    <record id="view_return_card_report_excel_export_form" model="ir.ui.view">
        <field name="name">return.card.report.excel.export.form</field>
        <field name="model">return.card.report.excel.export</field>
        <field name="type">form</field>
        <field name="arch" type="xml">
            <form string="Export File">
<!--                <group col="4" colspan="4">-->
                    <field name="report_file" readonly="1" filename="name" nolabel="1"/>
                    <field name="name" invisible="1"/>

<!--                </group>-->
                <footer>
                    <button name="action_back" string="_Back" type="object" class="oe_highlight"/>
                    or
                    <button string="Close" class="oe_highlight" special="cancel"/>
                </footer>
            </form>
        </field>
    </record>

    <record id="view_return_card_report_wizard_form" model="ir.ui.view">
        <field name="name">return.card.report.report.form</field>
        <field name="model">return.card.report</field>
        <field name="type">form</field>
        <field name="arch" type="xml">
            <form string="Refund Summary">
                <group>
                    <group>

                        <field name="date_present" required="1"/>
                        <field name="warehouse" required="0"/>
                        <field name="location" required="0"/>
<!--                        <field name="type"/>-->
                    </group>

                    <group>
                        <field name="category_id" required="0"/>
                        <field name="product" required="0"/>
                    </group>

                </group>
                <footer>
<!--                    <button name="print_report_pdf" string="Print(PDF)" type="object"  class="oe_highlight"/>-->
<!--                    or-->
                    <button name="print_report_excel" string="Print(Excel)" type="object" class="oe_highlight"/>
                    <button string="Cancel" class="oe_link" special="cancel"/>
                </footer>
            </form>
        </field>
    </record>

    <record id="action_return_card_report" model="ir.actions.act_window">
        <field name="name">รายงานสินค้าคงคลังสำหรับกรมศุลกากร</field>
        <field name="res_model">return.card.report</field>
        <field name="type">ir.actions.act_window</field>
        <field name="view_mode">form</field>
        <field name="view_id" ref="wizard_report_inventory.view_return_card_report_wizard_form"/>
        <field name="target">new</field>
    </record>

    แก้
    <menuitem id="return_card_report_id"
              action="action_return_card_report"
              parent="stock.menu_warehouse_report"
              name="รายงานสินค้าคงคลังสำหรับกรมศุลกากร"
              sequence="2"
    />


</odoo>