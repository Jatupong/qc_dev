<odoo>
    <record id="loyalty_program_form" model="ir.ui.view">
        <field name="name">loyalty.program.form</field>
        <field name="model">loyalty.program</field>
        <field name="inherit_id" ref="loyalty.loyalty_program_gift_ewallet_view_form"/>
        <field name="arch" type="xml">
            <field name="portal_point_name" position="after">
                <field name="valid_date"/>
                <field name="gift_card_value"/>
            </field>
            <field name="pos_config_ids" position="after">
                <field name="coupon_date"/>
            </field>

        </field>
    </record>

</odoo>