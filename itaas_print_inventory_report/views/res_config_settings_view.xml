<odoo>

    <record id="res_config_settings_view_form_stock_report" model="ir.ui.view">
        <field name="name">res.config.settings.view.form.stock.report</field>
        <field name="model">res.config.settings</field>
        <field name="inherit_id" ref="stock.res_config_settings_view_form"/>
        <field name="arch" type="xml">

            <xpath expr="//div[@data-key='stock']" position="inside">
                <h2 class="mt32">Reports</h2>
