                    <div class="col-12 col-lg-6 o_setting_box" id="internal_transfer_no_form">
                        <div class="o_setting_left_pane"/>
                        <div class="o_setting_right_pane">
                            <span class="o_form_label">Internal Transfer No. Form</span>
                            <div class="text-muted">
                                <field name="internal_transfer_no_form"/>
                            </div>
                        </div>
                    </div>
                    <div class="col-12 col-lg-6 o_setting_box" id="delivery_no_form">
                        <div class="o_setting_left_pane"/>
                        <div class="o_setting_right_pane">
                            <span class="o_form_label">Delivery No. Form</span>
                            <div class="text-muted">
                                <field name="internal_transfer_no_form"/>
                            </div>
                        </div>
                    </div>
                    <div class="col-12 col-lg-6 o_setting_box" id="receipt_no_form">
                        <div class="o_setting_left_pane"/>
                        <div class="o_setting_right_pane">
                            <span class="o_form_label">Receipt No. Form</span>
                            <div class="text-muted">
                                <field name="receipt_no_form"/>
                            </div>
                        </div>
                    </div>
