<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="view_stock_picking_form_print" model="ir.ui.view">
        <field name="name">stock.picking.form.print</field>
        <field name="model">stock.picking</field>
        <field name="inherit_id" ref="stock.view_picking_form"/>
        <field name="arch" type="xml">

            <xpath expr='//button[@name="%(stock.action_report_delivery)d"]' position='attributes'>
                <attribute name="name">%(delivery_order_report)d</attribute>
            </xpath>

        </field>
    </record>

</odoo>