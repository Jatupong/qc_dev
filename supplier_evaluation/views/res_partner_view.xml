<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="view_partner_evaluation_form_supplier_evaluation" model="ir.ui.view">
        <field name="name">view.partner.evaluation.form.supplier.evaluation</field>
        <field name="model">res.partner</field>
        <field name="inherit_id" ref="base.view_partner_form"/>
        <field name="arch" type="xml">
            <xpath expr="//notebook" position="inside">
                <page string="Supplier Assessment">
                    <field name ='supplier_evaluation_ids' readonly="1"/>
                </page>
            </xpath>
        </field>
    </record>

</odoo>