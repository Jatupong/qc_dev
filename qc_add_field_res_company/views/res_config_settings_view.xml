<odoo>

    <record id="res_config_settings_view_form_purchase_report" model="ir.ui.view">
        <field name="name">res.config.settings.view.form.purchase.report</field>
        <field name="model">res.config.settings</field>
        <field name="inherit_id" ref="purchase.res_config_settings_view_form_purchase"/>
        <field name="arch" type="xml">
<!--            //div[@data-key='purchase']/div/div[3]-->


            <xpath expr="//field[@name='document_id']" position="after">
                  <br/>
                  <field name="document_id5"/>
            </xpath>

            <xpath expr="//field[@name='document_id2']" position="after">
                  <br/>
                  <field name="document_id6"/>
            </xpath>



            <xpath expr="//div[@data-key='purchase']" position="inside">
<!--                <h2 class="mt32">Reports</h2>-->
                <div class="row mt16 o_settings_container" id="connectors_setting_container">
                    <div class="col-12 col-lg-6 o_setting_box" id="purchase_request_form">
                        <div class="o_setting_left_pane"/>
                        <div class="o_setting_right_pane">
                            <span class="o_form_label">Purchase Request</span>
                            <div class="text-muted">
                                <field name="purchase_request_form"/>
                                <br/>
                                <field name="document_id4"/>
                            </div>
                        </div>
                    </div>
                </div>
            </xpath>

        </field>
    </record>

</odoo>