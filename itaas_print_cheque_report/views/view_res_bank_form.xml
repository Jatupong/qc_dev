<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="res_bank_form_print_cheque" model="ir.ui.view">
            <field name="name">res.bank.form.print.cheque</field>
            <field name="model">res.bank</field>
            <field name="inherit_id" ref="base.view_res_bank_form"/>
            <field name="arch" type="xml">

                <xpath expr="//form/sheet" position="inside">
                    <group string="Layout">
                        <group>
                            <h5 colspan="2">Image1 A/C Payeeonly </h5>
                            <field name="layout_image1_top"/>
                            <field name="layout_image1_left"/>
                            <field name="layout_image1_height"/>
                            <field name="layout_image1_width"/>
                            <field name="layout_image1_image"/>
                            <field name="layout_image1_show"/>
                        </group>
                        <group>
                            <h5 colspan="2">Image2 //</h5>
                            <field name="layout_image2_top"/>
                            <field name="layout_image2_right"/>
                            <field name="layout_image2_height"/>
                            <field name="layout_image2_width"/>
                            <field name="layout_image2_image"/>
                            <field name="layout_image2_show"/>
                        </group>
                        <group>
                            <h5 colspan="2">Name of cheque</h5>
                            <field name="layout_name_top"/>
                            <field name="layout_name_left"/>
                            <field name="layout_name_show"/>
                        </group>
                        <group>
                            <h5 colspan="2">Partner name</h5>
                            <field name="layout_partner_top"/>
                            <field name="layout_partner_left"/>
                            <field name="layout_partner_show"/>
                        </group>
                        <group>
                            <h5 colspan="2">Date of cheque</h5>
                            <field name="layout_date_top"/>
                            <field name="layout_date_left"/>
                            <field name="layout_date_show"/>
                            <field name="layout_date_spacing"/>
                        </group>
                        <group>
                            <h5 colspan="2">Amount</h5>
                            <field name="layout_amount_top"/>
                            <field name="layout_amount_left"/>
                            <field name="layout_name_show"/>
                        </group>
                        <group>
                            <h5 colspan="2">Bath text</h5>
                            <field name="layout_baht_top"/>
                            <field name="layout_baht_left"/>
                            <field name="layout_baht_show"/>
                        </group>
                        <group>
                            <h5 colspan="2">Font</h5>
                            <field name="font_size"/>
                        </group>
                    </group>
                </xpath>

            </field>
        </record>

    </data>
</odoo>