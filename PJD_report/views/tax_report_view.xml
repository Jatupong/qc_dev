<odoo>

    <!-- tax report export form -->
    <record id="view_tax_report_wizard_form3" model="ir.ui.view">
        <field name="name">tax.report.form</field>
        <field name="model">tax.report</field>
        <field name="type">form</field>
        <field name="arch" type="xml">
            <form string="รายการส่งออกประจำเดือน">
                <group>
                    <field name="date_from" />

                    <field name="date_to" />
                </group>
                <group>
                    <field name="cuctomer"/>
                </group>
                <footer>
                    <button name="print_report_pdf2" string="Print(PDF)" type="object" default_focus="1" class="oe_highlight"/>
                    or
                    <button name="print_report2" string="Print(Excel)" type="object" default_focus="1" class="oe_highlight"/>
                    <button string="Cancel" class="oe_link" special="cancel"/>
                </footer>
            </form>
        </field>
    </record>


    <!-- Action for Tax Report Wizard -->

    <record id="action_tax_report_excel3" model="ir.actions.act_window">
        <field name="name">รายการส่งออกประจำเดือน</field>
        <field name="res_model">tax.report</field>
        <field name="type">ir.actions.act_window</field>
        <field name="view_mode">form</field>
        <field name="view_id" ref="view_tax_report_wizard_form3"/>
        <field name="target">new</field>
    </record>
<!--    <menuitem id="menu_main_tax_report" parent="account.menu_finance_reports" name="Thailand (Tax Report)"/>-->
    <menuitem id="menu_tax_report3" action="action_tax_report_excel3"  parent="account.menu_finance_reports"/>

</odoo>