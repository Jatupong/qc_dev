<odoo>


    <record id="view_tax_excel_export_form2" model="ir.ui.view">
        <field name="name">tax.excel.export.form2</field>
        <field name="model">tax.excel.export2</field>
        <field name="type">form</field>
        <field name="arch" type="xml">
            <form string="Export File 2">
                <group col="4" colspan="4">
                    <field name="report_file" readonly="1" filename="name" nolabel="1"/>
                    <field name="name" invisible="1"/>
                </group>
                <footer>
                    <button name="action_back" string="Back" type="object" class="oe_highlight"/>
                    or
                    <button string="Close" class="oe_highlight" special="cancel"/>
                </footer>
            </form>
        </field>
    </record>




    <record id="view_tax_report_wizard_form2" model="ir.ui.view">
        <field name="name">tax.report.form</field>
        <field name="model">tax.report</field>
        <field name="type">form</field>
        <field name="arch" type="xml">
            <form string="รายการส่งออกประจำเดือน">
                <group>
                    <field name="date_from" />

                    <field name="date_to" />
                </group>
                <group>
                    <field name="customer"/>
                </group>
                <footer>
                    <button name="print_report_pdf2" string="Print(PDF)" type="object" default_focus="1" class="oe_highlight"/>
                    or
                    <button name="print_report2" string="Print(Excel)" type="object" default_focus="1" class="oe_highlight"/>
                    <button string="Cancel" class="oe_link" special="cancel"/>
                </footer>
            </form>
        </field>
    </record>


    <record id="view_pnd53_report_wizard_form" model="ir.ui.view">
        <field name="name">pnd53.report.form</field>
        <field name="model">pnd53.report</field>
        <field name="type">form</field>
        <field name="arch" type="xml">
            <form string="PND Reports">
                <group col="4" colspan="4">
                    <field name="date_from"/>
                    <field name="date_to"/>
                    <field name="report_type"/>
                </group>
                <footer>
                    <button name="print_pnd53_report" string="Print" type="object" default_focus="1" class="oe_highlight"/>
                    or
                    <button name="print_pnd53_report_to_text" string="Print (Txt)" type="object" default_focus="1" class="oe_highlight"/>
                    or
                    <button string="Cancel" class="oe_link" special="cancel"/>
                </footer>
            </form>
        </field>
    </record>


    <record id="action_tax_report_excel2" model="ir.actions.act_window">
        <field name="name">รายการส่งออกประจำเดือน</field>
        <field name="res_model">tax.report</field>
        <field name="type">ir.actions.act_window</field>
        <field name="view_mode">form</field>
        <field name="view_id" ref="view_tax_report_wizard_form2"/>
        <field name="target">new</field>
    </record>

    <menuitem action="action_tax_report_excel2"
            id="menu_tax_report2"
            parent="account.menu_finance_reports"
            sequence="100"/>

</odoo>
