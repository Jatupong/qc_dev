            </field>
        </record>

        <record id="action_customer_channel" model="ir.actions.act_window">
            <field name="name">Customer Channel</field>
            <field name="res_model">res.channel.customers</field>
            <field name="type">ir.actions.act_window</field>
            <field name="view_mode">tree,form</field>
            <field name="view_id" ref="view_customer_channel_tree"/>
        </record>

        <menuitem id="menu_customer_channel"
                  name="Customer Channel"
                  parent="sale.menu_sale_general_settings"
                  action="action_customer_channel"/>

