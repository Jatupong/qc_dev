<?xml version="1.0" encoding="utf-8"?>
<odoo>
        <record id="view_customer_channel_form" model="ir.ui.view">
            <field name="name">view.customer.channel.form</field>
            <field name="model">res.channel.customers</field>
            <field name="arch" type="xml">
                <form>
                    <sheet>
                        <group>
                                <field name="name"/>
                        </group>

                    </sheet>
                </form>
            </field>
        </record>


